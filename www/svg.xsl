	<xsl:template name="rek">
		<xsl:param name="personID" />
		<xsl:param name="xCor" />
		<xsl:param name="yCor" />
		<xsl:param name="level" />
		<xsl:param name="oldPad" />
		<xsl:param name="parent" />
		<xsl:param name="noID" />
		
		<xsl:variable name="partnerID" >
			<xsl:for-each select="/familie/beziehungen/partner">
				<xsl:choose>
					<xsl:when test="@partnerEins = $personID">
						<text x="200" y="200">
							<xsl:value-of select="@partnerZwei" />
						</text>
					</xsl:when>
					<xsl:when test="@partnerZwei = $personID">
						<text x="200" y="200">
							<xsl:value-of select="@partnerEins" />
						</text>
					</xsl:when>
				</xsl:choose>
			</xsl:for-each>
		</xsl:variable>
		<xsl:variable name="podest">
			<xsl:for-each select="/familie/personen/person/@id">
				<xsl:choose>
					<xsl:when test="../@id=$partnerID">
						1
					</xsl:when>
				</xsl:choose>
			</xsl:for-each>
		</xsl:variable>
				
			<g>
			<xsl:attribute name="class">parent_<xsl:value-of select="$level"/></xsl:attribute>
			<xsl:variable name="kidCount" select="count(/familie/beziehungen/kind[@elternteil=$personID])"/>
			<!-- Beide Darstellen -->
			<xsl:for-each select="/familie/personen/person/@id">
				<xsl:choose>
					<xsl:when test="../@id=$personID">
						<xsl:call-template name="drawPerson">
							<xsl:with-param name="personID" select="../@id"/>
							<xsl:with-param name="xCor" select="$xCor"/>
							<xsl:with-param name="yCor" select="$yCor"/>
							<xsl:with-param name="level" select="$level"/>
							<xsl:with-param name="kid" select="1"/>
							<xsl:with-param name="parent" select="$parent"/>
						</xsl:call-template>
					</xsl:when>
					<xsl:when test="../@id=$partnerID">
						<xsl:call-template name="drawPartner">
							<xsl:with-param name="personID" select="../@id"/>
							<xsl:with-param name="xCor" select="$xCor + 90"/>
							<xsl:with-param name="yCor" select="$yCor"/>
							<xsl:with-param name="level" select="$level"/>
							<xsl:with-param name="kid" select="0"/>
							<xsl:with-param name="parent" select="$parent"/>
						</xsl:call-template>
						<!-- wo kommt das her???	
						<xsl:for-each select="/familie/beziehungen/kind[@kind=$partnerID]" >
							<xsl:call-template name="drawPerson">
								<xsl:with-param name="personID" select="26"/>
								<xsl:with-param name="xCor" select="$xCor + 400"/>
								<xsl:with-param name="yCor" select="$yCor - 100"/>
								<xsl:with-param name="level" select="$level"/>
								<xsl:with-param name="kid" select="0"/>
								<xsl:with-param name="parent" select="$parent"/>
							</xsl:call-template>
						</xsl:for-each>
						<xsl:variable name="Podest">1</xsl:variable>
						-->
						<!--Verbindung zwischen Partnern (x1 60 x2 90) -->
						<line x1="{$xCor+80}" y1="{$yCor}" x2="{$xCor+80}" y2="{$yCor}" style="stroke:rgb(255,255,255);stroke-width:2"/>
						<use x="{$xCor+45}" y="{$yCor -5}" xlink:href="#svgRing"/>
						
					</xsl:when>
				</xsl:choose>
			</xsl:for-each>
			
			<xsl:if test="$kidCount">				
				<xsl:choose>
					<xsl:when test="$podest = 1">
						<use x="{$xCor -5}" y="{$yCor+12}" xlink:href="#svgPodest"/>
					</xsl:when>
					<xsl:otherwise>
						<use x="{$xCor -55}" y="{$yCor+12}" xlink:href="#svgPodest"/>
					</xsl:otherwise>
				</xsl:choose>
			</xsl:if>
			
			
			<xsl:variable name="xPadKid" select="($oldPad)"/>
			
			<!--Anzahl der Kinder-->
			<xsl:if test="$kidCount &gt;= 0" >
				<xsl:choose>
					<xsl:when test="$podest = 1">
						<xsl:call-template name="drawConLine">
					<xsl:with-param name="xCor" select="$xCor + 75 - (($kidCount - 1)* $xPadKid *0.5)"/>
					<xsl:with-param name="yCor" select="$yCor +3"/>
					<xsl:with-param name="xMax" select="$xCor + 75 + (($kidCount - 1)* $xPadKid *0.5)"/>
					<xsl:with-param name="kidCount" select="$kidCount"/>
					<xsl:with-param name="xPadKid" select="$xPadKid"/>
				</xsl:call-template>
					</xsl:when>
					<xsl:otherwise>
						<xsl:call-template name="drawConLine">
						<xsl:with-param name="xCor" select="$xCor + 25 - (($kidCount - 1)* $xPadKid *0.5)"/>
						<xsl:with-param name="yCor" select="$yCor +3"/>
						<xsl:with-param name="xMax" select="$xCor + 25 + (($kidCount - 1)* $xPadKid *0.5)"/>
						<xsl:with-param name="kidCount" select="$kidCount"/>
						<xsl:with-param name="xPadKid" select="$xPadKid"/>
				</xsl:call-template>
					</xsl:otherwise>
				</xsl:choose>
			</xsl:if>
			<!--alle Kinder darstellen mit rekursion;-) -->
			
			<xsl:for-each select="/familie/beziehungen/kind[@elternteil=$personID]">
				<xsl:choose>
					<xsl:when test="$podest = 1">
						<xsl:call-template name="rek">
					<xsl:with-param name="personID" select="@kind"/>
					<xsl:with-param name="xCor" select="$xCor +50 - (($kidCount - 1)* ($xPadKid *0.5))+((position() - 1) * $xPadKid)"/>
					<xsl:with-param name="yCor" select="$yCor +100"/>
					<xsl:with-param name="level" select="$level + 1"/>
					<xsl:with-param name="oldPad" select="$xPadKid"/>
					<xsl:with-param name="parent" select="$personID"/>
					<!-- {$xCor-(($kidCount*100)+200)+(position()*200)}-->
				</xsl:call-template>
					</xsl:when>
					<xsl:otherwise>
						<xsl:call-template name="rek">
					<xsl:with-param name="personID" select="@kind"/>
					<xsl:with-param name="xCor" select="$xCor - (($kidCount - 1)* ($xPadKid *0.5))+((position() - 1) * $xPadKid)"/>
					<xsl:with-param name="yCor" select="$yCor +100"/>
					<xsl:with-param name="level" select="$level + 1"/>
					<xsl:with-param name="oldPad" select="$xPadKid"/>
					<xsl:with-param name="parent" select="$personID"/>
					<!-- {$xCor-(($kidCount*100)+200)+(position()*200)}-->
				</xsl:call-template>
					</xsl:otherwise>
				</xsl:choose>
			</xsl:for-each>
			</g>
			
	</xsl:template>

